    matchLabels:
      app: mavenassur
  template:
    metadata:
      labels:
        app: mavenassur
    spec:
      containers:
      - name: mavenassur
        image: hnouri86/mavenassur:1.0.20240127
        ports:
        - containerPort: 8080 
        command: ["sh", "-c", "sleep 3600"]  
